{
  "manifest_version": 3,
  "name": "kurisu",
  "description": "kurisu",
  "version": "1.0",
  "content_scripts": [
    {
      "js":[
        "js/kurisu.js"
      ],
      "matches": ["https://*/*","http://*/*"]
    }
  ],
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  }
}